{
  "name": "shadcn-theme-maker",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "db:generate": "drizzle-kit generate --config=./src/db/drizzle.config.ts",
    "db:generate:custom": "drizzle-kit generate --config=./src/db/drizzle.config.ts --custom",
    "db:migrate": "drizzle-kit migrate --config=./src/db/drizzle.config.ts",
    "db:studio": "drizzle-kit studio --config=./src/db/drizzle.config.ts",
    "db:seed": "tsx --env-file=.env src/scripts/seed-db.ts",
    "postinstall": "sh -c 'if [ \"$NODE_ENV\" != \"production\" ]; then if [ ! -d .screenshot-service ]; then git clone https://github.com/miketromba/screenshot-service.git .screenshot-service; fi && cd .screenshot-service && pnpm docker:build; fi'",
    "ss:dev": "docker run --rm -it --add-host=host.docker.internal:host-gateway -p 3006:3000 -e NODE_ENV=development -e AUTH_TOKEN=dev-token -e HOST_WHITELIST=host.docker.internal screenshot-service"
  },
